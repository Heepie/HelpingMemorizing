<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.heepie.helpingmemorizingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MaterialAnimations">

        <activity android:name="com.heepie.helpingmemorizingapp.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.heepie.helpingmemorizingapp.view.TestActivity" />
        <activity android:name="com.heepie.helpingmemorizingapp.view.CheckFolderActivity" />
        <activity android:name="com.heepie.helpingmemorizingapp.view.CreateCardActivity" />
        <activity android:name="com.heepie.helpingmemorizingapp.view.CreateFolderActivity"></activity>
    </application>

</manifest>